{
  "mcpClient": {
    "provider": "openai",
    "model": "gpt-4o-mini",
    "api_key": "${env:OPENAI_API_KEY}"
  },
  "mcpServers": {
    "remoteMCP": {
      "type": "http",
      "url": "https://mcp.remote-mcp.com"
    },
    "dice": {
      "type": "stdio",
      "command": "node",
      "args": ["examples/dice-mcp-server.js"]
    }
  }
}