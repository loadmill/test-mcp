{
  "mcpClient": {
    "provider": "openai",
    "model": "gpt-4o-mini",
    "api_key": "${env:OPENAI_API_KEY}"
  },
  "mcpServers": {
    "dice": {
      "type": "stdio",
      "command": "node",
      "args": ["examples/dice-mcp-server.js"]
    },
    "coin": {
      "type": "stdio",
      "command": "node", 
      "args": ["examples/coin-mcp-server.js"]
    }
  }
}