{
  "mcpClient": {
    "provider": "anthropic",
    "model": "claude-3-5-haiku-latest",
    "api_key": "${env:ANTHROPIC_API_KEY}"
  },
  "mcpServers": {
    "dice": {
      "type": "stdio",
      "command": "node",
      "args": ["examples/dice-mcp-server.js"]
    },
    "remoteMCP": {
      "type": "http",
      "url": "https://mcp.remote-mcp.com"
    }
  }
}