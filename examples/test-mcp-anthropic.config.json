{
  "mcpClient": {
    "provider": "anthropic",
    "model": "claude-3-5-haiku-latest",
    "api_key": "${env:ANTHROPIC_API_KEY}"
  },
  "mcpServers": {
    "dice": {
      "type": "stdio",
      "command": "node",
      "args": ["examples/dice-mcp-server.js"]
    },
    "coin": {
      "type": "stdio",
      "command": "node", 
      "args": ["examples/coin-mcp-server.js"]
    }
  }
}